{
    "buildCommand": "cd client && yarn install && ./node_modules/vite/bin/vite.js build",
    "outputDirectory": "client/dist",
    "framework": "vite",
    "rewrites": [
      {
        "source": "Airnab-Clone/api/(.*)",
        "destination": "Airnab-Clone/api/index.js"
      },
      {"source": "/(.*)", "destination": "/"}
    ]
  }